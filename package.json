{
  "name": "monitor_full_stack",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "packageManager": "pnpm@9.15.4",
  "scripts": {
    "lint": "eslint",
    "lint:staged": "lint-staged",
    "commit": "git-cz",
    "prepare": "husky",
    "clean": "run-s clean:build clean:caches",
    "clean:build": "pnpm --filter @@monitor_full_stack/* clean",
    "clean:caches": " rimraf .turbo/cache **/*/.turbo",
    "clean:deps": "rimraf node_modules **/*/node_modules",
    "clean:all": "run-s clean:build clean:caches clean:deps",
    "build": "turbo build",
    "build:watch": "turbo build:watch",
    "dev": "turbo dev",
    "docker:start": "docker compose  -p lwh-monitor-demo -f .devcontainer/docker-compose.yml up -d",
    "docker:stop": "docker compose  -p lwh-monitor-demo -f .devcontainer/docker-compose.yml down",
    "server:build": "pnpm --filter @monitor_full_stack/dsn_monitor build:server",
    "server:dev": "pnpm  --filter @monitor_full_stack/dsn_monitor start:dev"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss,md}": [
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@eslint/js": "9.39.1",
    "@types/node": "22.5.5",
    "commitizen": "4.3.1",
    "cz-git": "^1.12.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "0.4.24",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "fast-glob": "3.3.3",
    "globals": "16.5.0",
    "husky": "9.1.7",
    "lint-staged": "16.2.7",
    "typescript": "5.9.3",
    "typescript-eslint": "8.48.1",
    "tsup": "8.3.0",
    "turbo": "2.6.3",
    "rimraf": "6.1.2",
    "npm-run-all2": "8.0.4"
  },
  "dependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/vue": "^2.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "vue": "^3.0.0"
  }
}